{
  "name": "Swetrix",
  "version": "0.0.0",
  "description": "Swetrix Analytics Extension",
  "homepage_url": "https://swetrix.com",
  "manifest_version": 2,
  "minimum_chrome_version": "74",
  "applications": {
    "gecko": {
      "id": "extension@swetrix.com",
      "strict_min_version": "67.0"
    }
  },
  "icons": {
    "512": "assets/icon.png"
  },
  "permissions": [
    "cookies"
  ],
  "browser_action": {
    "default_icon": "assets/icon.png",
    "default_popup": "src/static/popup.html"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "src/scripts/background.js"
    ]
  }
}
